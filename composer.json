{
    "name": "servexs/barcode-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Zend_Barcode and PHP QR Lib to be easily used in symfony2 via twig",
    "keywords": ["barcode", "qr code", "zend", "twig"],
    "authors": [
        {
            "name": "Philipp A. Mohrenweiser",
            "email": "phiamo@googlemail.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/phiamo/MopaBootstrapBundle/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mopa\\Bundle\\BarcodeBundle\\": "src",
            "Mopa\\BarcodeBundle\\Tests\\": "Tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^3.0|^4.4",
        "symfony/twig-bundle": "~3.1|^4.4",
        "symfony/form": "~3.1|^4.4",
        "liip/imagine-bundle": "^1.8.0|^2.0",
        "laminas/laminas-barcode": "^2.6",
        "laminas/laminas-validator": "^2.8",
        "laminas/laminas-servicemanager": "^3.1",
        "twig/twig": "~1.14",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "symplify/easy-coding-standard": "^10.2"
    },
    "suggest": {
        "symfony/monolog-bundle": "^1.0"
    },
    "scripts": {
        "tests": "phpunit tests",
        "lint-dry": "ecs check src",
        "lint": "ecs check src --fix"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x"
        }
    }
}
