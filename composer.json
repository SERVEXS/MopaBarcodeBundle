{
    "name": "mopa/barcode-bundle",
    "type": "symfony-bundle",
    "description": "Integrates Zend_Barcode and PHP QR Lib to be easily used in symfony2 via twig",
    "keywords": ["barcode", "qr code", "zend", "twig"],
    "authors": [
        {
            "name": "Philipp A. Mohrenweiser",
            "email": "phiamo@googlemail.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/phiamo/MopaBootstrapBundle/contributors"
        }
    ],
    "autoload": {
        "psr-0": {
            "Mopa\\Bundle\\BarcodeBundle": ""
        }
    },
    "target-dir": "Mopa/Bundle/BarcodeBundle",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=3.0",
        "symfony/twig-bundle": "~2.3|~3.1",
        "liip/imagine-bundle": "1.8.0",
        "zendframework/zend-barcode": "^2.6",
        "zendframework/zend-validator": "^2.8",
        "zendframework/zend-servicemanager": "^3.1"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.6"
    },
    "suggest":  {
        "symfony/monolog-bundle": "^1.0"
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.6.x"
        }
    }
}

